
<radio-group class="radio-group" onChange="radioChange" name="第{{titleNum}}题">
  <view class="radio-title">{{titleNum}}、【单选】 {{examSubject.title}}</view>
  <label class="radio" a:for="{{examSubject.option}}" key="label-{{index}}">
    <radio value="{{examSubject.option[index].name}}" checked="{{examSubject.option[index].checked}}"/>
    <text class="radio-text">{{examSubject.option[index].value}}</text>
  </label>
</radio-group>